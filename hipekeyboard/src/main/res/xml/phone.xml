<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
          android:horizontalGap="@dimen/key_horizontal_gap"
          android:keyHeight="@dimen/key_height_medium"
          android:keyWidth="@dimen/key_width_huge"
          android:verticalGap="@dimen/key_vertical_gap">
    <Row>
        <Key
                android:keyEdgeFlags="left"
                android:keyLabel="1"/>
        <Key android:keyLabel="2"/>
        <Key android:keyLabel="3"/>
        <Key
                android:keyEdgeFlags="right"
                android:keyLabel="#"/>
    </Row>

    <Row>
        <Key
                android:keyEdgeFlags="left"
                android:keyLabel="4"/>
        <Key android:keyLabel="5"/>
        <Key android:keyLabel="6"/>
        <Key
                android:keyEdgeFlags="right"
                android:keyLabel="+"/>
    </Row>

    <Row>
        <Key
                android:keyEdgeFlags="left"
                android:keyLabel="7"/>
        <Key android:keyLabel="8"/>
        <Key android:keyLabel="9"/>
        <Key
                android:codes="-5"
                android:isRepeatable="true"
                android:keyEdgeFlags="right"
                android:keyIcon="@drawable/ic_delete_white"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
                android:keyEdgeFlags="left"
                android:keyLabel="\?123"/>
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <!--<Key-->
        <!--android:codes="-101"-->
        <!--android:keyIcon="@drawable/ic_language_24dp" />-->
        <Key android:keyLabel="0"/>

        <Key
                android:codes="32"
                android:isRepeatable="true"/>
        <Key
                android:codes="10"
                android:keyEdgeFlags="right"
                android:keyIcon="@drawable/ic_next_circle_filled_45dp"/>
    </Row>
</Keyboard>